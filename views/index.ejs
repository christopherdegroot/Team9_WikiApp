<!DOCTYPE html>
<html lang="en">

<head>
  <title>Dashboard</title>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css"
    type="text/css" />
  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
  <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
  <link rel="stylesheet" href="/styles/main.css" type="text/css" />
  <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

  <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
  <script type="text/javascript" src="/scripts/dashboard.js"></script>
  <script type="text/javascript" src="/scripts/app.js"></script>


</head>



<body>
  <!-- side toggle bar -->
  <span id="side-nav">
    <div class="nav-content">
      <div>
        <!-- submit button -->
        <form method="GET" action="/">
          <button type="submit">Dashboard</button>
        </form>
      </div>
      <br>
      <div >
        <!-- profile button -->
        <form method="GET" action="/profile">
          <button type="submit">Profile</button>
        </form>
      </div>
    </div>
  </span>


  <main id="container">

    <!-- top navbar -->
    <header class="top-nav">
      <span class="expand">
        <i class="fa-solid fa-grip-lines"></i>
      </span>
      <form>
        <button>Drop a üìç</button>
      </form>
    </header>

    <!-- google map -->
    <span class="map-container">
      <div id="map_canvas" style="width:100%; height:75.5vh; border-radius: 10px;"></div>
        <!-- Async script executes immediately and must be after any DOM elements used in callback. -->
        <script
          src="https://maps.googleapis.com/maps/api/js?key=<%=process.env.DB_GOOGLE_API%>&callback=initMap&libraries=&v=weekly"
          async>
          </script>

    </span>
  </main>
</body>
</html>
